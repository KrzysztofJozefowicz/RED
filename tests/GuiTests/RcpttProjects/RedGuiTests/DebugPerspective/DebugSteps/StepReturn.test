Entry-Name: .content

with [get-view "Project Explorer" | get-tree] {
    select ScriptWithResource | select ScriptWithResource | double-click
    select "ScriptWithResource/BigScript.robot" | select "ScriptWithResource/BigScript.robot" | double-click
}
with [get-editor "BigScript.robot" | get-left-ruler | get-ruler-column AnnotationColumn] {
    double-click-ruler 17
    double-click-ruler 42
}

exec-with-options -disableJobWaiting -command {
	get-view "Project Explorer" | get-tree | select "ScriptWithResource/BigScript.robot" | get-menu "Debug As/1 Robot Test" 
	    | click
	    
	wait-for-thread -file "BigScript.robot"
	
	check-debug-marker-on-ruler -line 17 -script_name "BigScript.robot" 
	get-button "Step Return (F7)" | get-property enablement | equals true | verify-true
	
	//go to keyword
	get-button "Step Into (F5)" | click
	wait-for-specific-nr-of-threads -nr_items_in_thread 4
	get-button "Step Into (F5)" | click
	check-debug-marker-on-ruler -line 97 -script_name "BigScript.robot"
	
	//return to test (this is last line so go back to Teardown)
	get-button "Step Return (F7)" | click
	wait-for-specific-nr-of-threads -nr_items_in_thread 1
	check-debug-marker-on-ruler -line 6  -script_name "BigScript.robot"
	
	//resume: go to next user keyword
	get-button "Resume (F8)" | click
	wait-for-specific-nr-of-threads -nr_items_in_thread 1
	check-debug-marker-on-ruler -line 42  -script_name "BigScript.robot"
	get-button "Step Into (F5)" | click
	wait-for-specific-nr-of-threads -nr_items_in_thread 2
	check-debug-marker-on-ruler -line 82  -script_name "BigScript.robot"
	
	//return - will go back to last line of test
	get-button "Step Return (F7)" | click
	wait-for-specific-nr-of-threads -nr_items_in_thread 1
	check-debug-marker-on-ruler -line 43  -script_name "BigScript.robot"
	
	//check that Return is disabled
	get-button "Step Return (F7)" | get-property enablement | equals false | verify-true

	get-button "Resume (F8)" | click
	

}
