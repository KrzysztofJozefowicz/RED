Content-Type: text/ecl
Entry-Name: .content

get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" 
    | double-click
    
with [get-editor "TestFile.robot"] {
    with [get-text-viewer] {
        key-type "M1+a"
        key-type Del
        key-type Enter -times 2
        key-type Up -times 2
        key-type "M1+SPACE"
        key-type Down
		key-type Enter 
		
		
		repeat [val index] -times 12 -command{
			key-type "M1+SPACE"
			key-type Down -times $index
	        key-type Enter -times 2		
		}
		
        type-text Met
        key-type "M1+SPACE"    
       	type-text "second Metadata"
        key-type Enter
       
        key-type "M1+SPACE"
        type-text T
        key-type Down -times 2
        key-type Enter
    }
}

get-editor "TestFile.robot" | get-text-viewer | get-property text | equals "*** Settings ***\n"
    + "Library  \n"
    + "Resource  \n"
    + "Variables  \n"
    + "Documentation  \n"
    + "Metadata  \n"
    + "Suite Setup  \n"
    + "Suite Teardown  \n"
    + "Force Tags  \n"
    + "Default Tags  \n"
    + "Test Setup  \n"
    + "Test Teardown  \n"
    + "Test Template  \n"
    + "Metadata  second Metadata\n"
    + "Test Template" | verify-true


    
wait 3000


with [get-view Outline | get-tree] {
    get-item "TestFile.robot/Settings" | get-property childCount | equals 13 | verify-true
    get-item "TestFile.robot/Settings/Documentation" | get-property index | equals 0 | verify-true
    get-item "TestFile.robot/Settings/Suite Setup" | get-property index | equals 1 | verify-true
    get-item "TestFile.robot/Settings/Suite Teardown" | get-property index | equals 2 | verify-true
    get-item "TestFile.robot/Settings/Test Setup" | get-property index | equals 3 | verify-true
    get-item "TestFile.robot/Settings/Test Teardown" | get-property index | equals 4 | verify-true
    get-item "TestFile.robot/Settings/Force Tags" | get-property index | equals 5 | verify-true
    get-item "TestFile.robot/Settings/Default Tags" | get-property index | equals 6 | verify-true
    get-item "TestFile.robot/Settings/Test Template" | get-property index | equals 7 | verify-true
    get-item "TestFile.robot/Settings" | get-item "Test Template" -index 1 | get-property index | equals 8 | verify-true
    get-item "TestFile.robot/Settings/Metadata" | get-property index | equals 9 | verify-true
}
