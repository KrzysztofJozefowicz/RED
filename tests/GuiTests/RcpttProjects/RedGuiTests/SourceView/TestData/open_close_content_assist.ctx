Entry-Name: .ecl.context

proc "close-content-assist" [val file_name]{
	with [get-editor $file_name]{
	    get-canvas | key-type "TRAVERSE_ESCAPE"
	    get-text-viewer | key-type ESC
	}
}

proc "open-content_assist" [val file_name] [val remove_previouse_char 0] [val first_char ""] [val row] [val col]{
	with [get-editor $file_name] {
	    with [get-text-viewer] {
	        set-caret-pos $row $col
	        if [$remove_previouse_char| equals 1]{
	        	key-type BackSpace
	        }
	        type-text $first_char
	        key-type "M1+SPACE"
	        wait 200
	    }
	}
}
