    get-property "getItems().TableItem[1].getText()" | equals "Change to '..${/}empty_file/empty1/empty2/empty.py'" // !!!!!!!!!!!!! << to change
        | verify-true
}
get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table 
