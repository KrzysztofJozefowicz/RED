// QF chagnes path to correct
get-view "Project Explorer" | get-tree | select "RobotProject/Script.robot" | double-click
// remove content of Source
with [get-editor "Script.robot" | get-text-viewer] {
    set-caret-pos 11 19
    key-type "M1+a"
    key-type Del
}
