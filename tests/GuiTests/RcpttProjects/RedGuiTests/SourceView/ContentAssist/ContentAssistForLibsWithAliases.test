// open project
with [get-view "Project Explorer" | get-tree] {
    select AliasProject | double-click
    select "AliasProject/TestSuite.robot" | double-click
}

// enter settings: libraries and resource
with [get-editor "TestSuite.robot" | get-text-viewer] {
    key-type Enter -times 2
    key-type Up -times 2
    type-text "*** Settings"
    key-type "M1+SPACE"
    wait 200
    key-type Enter
    
    //library1
    type-text Libra
    key-type "M1+SPACE"
    wait 200
    key-type Enter
    type-text libra
    key-type Enter -times 2
    
    //library 2 iwth alias
    type-text Lib
    key-type "M1+SPACE"
    wait 200
    key-type Enter
    type-text library2
    key-type Enter
    type-text WIT
    key-type "M1+SPACE"
    wait 200
    key-type Enter
    type-text alias
    key-type Enter
    
    //resource
    type-text Res
    key-type "M1+SPACE"
    wait 200
    key-type Enter
    type-text "res/"
    key-type Enter -times 2
    
    // enter Suite Setup for library
    type-text Su
    key-type "M1+SPACE"
    wait 200
    key-type Enter
    type-text library1
}
