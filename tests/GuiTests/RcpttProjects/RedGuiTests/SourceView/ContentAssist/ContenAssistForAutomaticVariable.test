//open file
with [get-view "Project Explorer" | get-tree] {
    double-click
    select "RobotProject/TestFile.robot" | double-click
}
//open content assist in Test
with [get-editor "TestFile.robot" | get-text-viewer] {
    set-caret-pos 1 19
    key-type  Enter
    type-text test
    key-type Enter
    type-text "$"
    key-type "M1+SPACE"
    wait 500
}
