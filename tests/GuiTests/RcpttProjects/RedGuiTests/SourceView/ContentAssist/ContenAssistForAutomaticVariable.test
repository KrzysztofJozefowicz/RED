//Open editor
get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | select "RobotProject/TestFile.robot" 
    | double-click


//---------------------------------------------------- Settings ----------------------------------------------------------
//check variables on Setting section    
open-content_assist -file_name "TestFile.robot" -row 2 -col 14 -first_char "$"
check-automatic-variables-for-settings
close-content-assist -file_name  "TestFile.robot"

//check list
open-content_assist -file_name "TestFile.robot" -remove_previouse_char 1 -row 2 -col 15 -first_char "@"
check-automatic-lists
close-content-assist -file_name  "TestFile.robot"

//check maps
open-content_assist -file_name "TestFile.robot" -remove_previouse_char 1 -row 2 -col 15 -first_char "&"
check-automatic-maps
close-content-assist -file_name  "TestFile.robot"



//check all varibales, maps, list on Variables
open-content_assist -file_name "TestFile.robot" -remove_previouse_char 0 -row 4 -col 12 -first_char ""
check-automatic-variables-for-settings -count_items 0
check-automatic-maps -start_index 31 -count_items 0
check-automatic-lists  -start_index 33 -count_items 0

get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table | get-property itemCount | equals 35 
    | verify-true
    
close-content-assist -file_name  "TestFile.robot"    
    
//------------------------Test cases -----------------------------------------------------------------    

//check variables
open-content_assist -file_name "TestFile.robot" -row 9 -col 4 -first_char "$"
check-automatic-variables-in-test-cases
close-content-assist -file_name "TestFile.robot" 

//check list
open-content_assist -file_name "TestFile.robot" -remove_previouse_char 1 -row 9 -col 5 -first_char "@"
check-automatic-lists
close-content-assist -file_name "TestFile.robot" 

//check maps
open-content_assist -file_name "TestFile.robot" -remove_previouse_char 1 -row 9 -col 5 -first_char "&"
check-automatic-maps
close-content-assist  -file_name "TestFile.robot"  

//----------------------------- keywords ------------------------------------------------

open-content_assist -file_name "TestFile.robot" -row 13 -col 5 -first_char "$"
check-automatic-variables-in-keywords
close-content-assist -file_name "TestFile.robot" 

//check list
open-content_assist -file_name "TestFile.robot" -remove_previouse_char 1 -row 13 -col 6 -first_char "@"
check-automatic-lists
close-content-assist -file_name "TestFile.robot" 

//check maps
open-content_assist -file_name "TestFile.robot" -remove_previouse_char 1 -row 13 -col 6 -first_char "&"
check-automatic-maps
close-content-assist  -file_name "TestFile.robot"  
//
//----------------------------- Variables ------------------------------------------------
get-editor "TestFile.robot" | get-text-viewer | set-caret-pos 16 1
get-editor "TestFile.robot" | get-text-viewer | type-text "*** Variables ***\n"
    + "${newScalar}\t"
wait 1000    
open-content_assist -file_name "TestFile.robot" -row 17 -col 15 -first_char "$"
check-automatic-variables-in-variables -start_index 1
close-content-assist -file_name "TestFile.robot" 

//check list
open-content_assist -file_name "TestFile.robot" -remove_previouse_char 1 -row 17 -col 16 -first_char "@"
with [get-editor "TestFile.robot" | get-text-viewer] {
    key-type "M1+SPACE"
}
with [get-window -from "CompletionProposalPopup.createProposalSelector()" | get-table] {
    get-property itemCount | equals 1 | verify-true
    get-property "getItems().TableItem[0].getText()" | equals "@{EMPTY}" | verify-true
}
close-content-assist -file_name "TestFile.robot" 

//check maps
open-content_assist -file_name "TestFile.robot" -remove_previouse_char 1 -row 17 -col 16 -first_char "&"
check-automatic-maps
close-content-assist  -file_name "TestFile.robot" 


