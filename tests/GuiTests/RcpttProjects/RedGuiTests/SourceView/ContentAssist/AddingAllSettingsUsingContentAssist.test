Content-Type: text/ecl
Entry-Name: .content

with [get-view "Project Explorer" | get-tree] {
    select RobotProject | select RobotProject | double-click
    select "RobotProject/TestFile.robot" | select "RobotProject/TestFile.robot" | double-click
}

with [get-editor "TestFile.robot" | get-text-viewer] {
	//remove all
    set-caret-pos 1 1
    key-type "M1+a"
    key-type Del
    //add Settings 
    key-type "M1+SPACE"
    key-type Down
    key-type Enter
    
    //open contetnt asiist and check it
    key-type "M1+SPACE"
	
	check-content-assist-for-settings
	key-type Esc
}


