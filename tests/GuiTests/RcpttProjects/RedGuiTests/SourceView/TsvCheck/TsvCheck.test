// tests to check multiline comment, tripple-dots, loops which caused NPEs in pre 0.6
get-tab-folder -index 3 | get-tab-item Problems | click
get-view Problems | get-label "6 errors, 0 warnings, 0 others" | get-property caption 
    | equals "6 errors, 0 warnings, 0 others" | verify-true
    
with [get-view Problems | get-tree] {
    get-item "Errors \\(6 items\\)" | get-property "getItems().TreeItem[0].getText()" 
        | equals "Invalid number of arguments. Keyword 'log' expects from 1 to 5 arguments, but 0 are provided" 
        | verify-true
    get-item "Errors \\(6 items\\)" | get-property "getItems().TreeItem[1].getText()" 
        | equals "Test case 'T3' contains no keywords to execute" | verify-true
    get-item "Errors \\(6 items\\)" | get-property "getItems().TreeItem[2].getText()" 
        | equals "There is no keyword to execute specified" | verify-true
    get-item "Errors \\(6 items\\)" | get-property "getItems().TreeItem[3].getText()" | equals "Unknown keyword ''" 
        | verify-true
    get-item "Errors \\(6 items\\)" | get-property "getItems().TreeItem[4].getText()" | equals "Unknown keyword ''" 
        | verify-true
    get-item "Errors \\(6 items\\)" | get-property "getItems().TreeItem[5].getText()" | equals "Unknown keyword '\\'" 
        | verify-true
}    
