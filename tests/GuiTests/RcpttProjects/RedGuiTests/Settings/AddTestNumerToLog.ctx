let [val temp [substitute-variables "${system_property:RCPPT_projectPath}"]]{
	if [$temp | equals ""] {
		global [val RCPTT_project_path $user_RCPTT_project_path]
	}-else{
		global [val RCPTT_project_path $temp]
	}
}

global [val TestNameAndDesc [read-lines -uri [concat $RCPTT_project_path "/Logs/counter.txt"]]]
concat "---------------- Test: " $TestNameAndDesc " ---------------" |log 
//concat "---------------- Test: " $TestNameAndDesc ". " [get-time | format-time -format "HH:mm:ss:S"] " ---------------" |log 
