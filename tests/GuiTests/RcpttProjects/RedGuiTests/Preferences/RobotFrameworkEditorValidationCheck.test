Content-Type: text/ecl
Entry-Name: .content

// simple check if Validation menu exists

// open preferences
get-preferences-menu | click
get-window Preferences | get-tree | select "Robot Framework/Validation"
// check if Validation is present in item tree and opened page has caption Validation
with [get-window Preferences] {
    get-tree | get-item "Robot Framework/Validation" | get-property caption | equals Validation | verify-true
    get-label Validation | get-property caption | equals Validation | verify-true
}
get-window Preferences | get-button OK | click
