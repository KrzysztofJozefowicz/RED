// Edit file
get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | double-click
get-editor "TestFile.robot" | get-tab-folder | get-tab-item Variables | click


get-editor "TestFile.robot" | get-link "Section is not yet defined, do you want to create it?" | click
// Create files

with [get-editor "TestFile.robot" | get-table] {
    get-cell 0 0 | double-click
    get-cell 0 0 | mouse down -count 2
    double-click
    select "${var}" | activate-cell-edit
    with [get-editbox] {
        click-text 3 6
        key-type "NUMPAD_ENTER" "\\u000D"
    }
    apply-cell-edit -deactivate
    key-type "M1+s"
}


with [get-editor "TestFile.robot" | get-table] {
    get-cell 1 0 | double-click
    get-cell 1 0 | mouse down -count 2
    double-click
    select "${var1}" | activate-cell-edit
    with [get-editbox] {
        click-text 3 7
        key-type "NUMPAD_ENTER" "\\u000D"
    }
    apply-cell-edit -deactivate
    key-type "M1+s"
}
// Open history
get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | get-menu "Team/Show Local History" 
    | click
// Open each revision
get-view History | get-tree | select [get-item ".*" -index 0] | double-click
get-view History | get-tree | select [get-item ".*" -index 1] | double-click
get-view History | get-tree | select [get-item ".*" -index 2] | double-click


get-view History | get-tree | select [get-item ".*" -index 0] | double-click



get-editor "TestFile.robot" | get-table | get-property itemCount | equals 3 | verify-true

get-view History | get-tree | select [get-item ".*" -index 1] | double-click
get-editor "TestFile.robot [/RobotProject/TestFile.robot]" | get-tab-folder | get-tab-item Variables | click
get-editor "TestFile.robot [/RobotProject/TestFile.robot]" | get-table | get-property itemCount | equals 2 | verify-true

get-view History | get-tree | select [get-item ".*" -index 2] | double-click
with [get-editor "TestFile.robot [/RobotProject/TestFile.robot]" -index 1] {
    get-tab-folder | get-tab-item Variables | click
}
get-editor "TestFile.robot [/RobotProject/TestFile.robot]" -index 1 | get-table | get-property itemCount | equals 0 
    | verify-true
