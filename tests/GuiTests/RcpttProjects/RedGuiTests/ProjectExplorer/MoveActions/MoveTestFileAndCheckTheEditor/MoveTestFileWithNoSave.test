Content-Type: text/ecl
Entry-Name: .content

get-view "Project Explorer" | get-tree | select RobotProject | get-menu "New/Robot Test Suite" | click
with [get-window "New Robot Test Suite"] {
    with [get-editbox -after [get-label "File name:"]] {
        set-text "TestFileToMove.robot"
    }
    get-button Finish | click
}


with [get-editor "TestFileToMove.robot"] {
    get-tab-folder | get-tab-item Variables | click
    get-link "Section is not yet defined, do you want to create it?" | click
}

with [get-editor "TestFileToMove.robot" | get-table] {
    get-cell 0 0 | double-click
    get-cell 0 0 | mouse down -count 2
    double-click
    select "${var}" | activate-cell-edit
    with [get-editbox] {
        click-text 3 6
        key-type "NUMPAD_ENTER" "\\u000D"
    }
    apply-cell-edit -deactivate
}

with [get-editor "TestFileToMove.robot" | get-table] {
    get-cell 0 1 | click
    get-cell 0 1 | mouse down -count 2
    double-click
    select "${var}" | activate-cell-edit -column 1
    with [get-editbox] {
        set-text TestValue
        key-type Enter
    }
    apply-cell-edit -deactivate
}
with [get-view "Project Explorer" | get-tree] {
    select "RobotProject/TestFileToMove.robot"
    get-item "RobotProject/TestFileToMove.robot" | drag-start 38 7
    get-item "RobotProject/TestFile.robot" | drag-enter 39 15 -detail move
    get-item "RobotProject/TestFile.robot" | drag-over 40 0 -detail copy
    get-item "RobotProject/TestSuiteFolder" | drag-over 41 7 -detail move
    get-item "RobotProject/TestFile.robot" | drag-exit
    get-item "RobotProject/TestSuiteFolder" | drag-accept 41 7 -detail move
    drag-set-data
    get-item "RobotProject/TestSuiteFolder" | drop 41 7 -detail move
    drag-end -detail move
}
get-window "Save Resource" | get-button Yes | click
with [get-editor "TestFileToMove.robot" | get-table] {
    get-cell 0 1 | click
    get-cell 0 1 | mouse down -count 2
    double-click
    select "${var}" | activate-cell-edit -column 1
    with [get-editbox] {
        set-text TestValue2
        key-type "NUMPAD_ENTER" "\\u000D"
    }
    apply-cell-edit -deactivate
}
get-view "Project Explorer" | get-tree | select "RobotProject/TestSuiteFolder/TestFileToMove.robot" 
    | get-menu "Open With/Robot Table Editor" | click
get-editor "TestFileToMove.robot" -index 1 | get-tab-folder | get-tab-item Variables | click
get-editor "TestFileToMove.robot" -index 1 | get-table | get-property "columns[0].index" | equals 0 | verify-true
get-view "Project Explorer" | get-tree | select "RobotProject/TestSuiteFolder"
get-view "Project Explorer" | get-tree | get-item "RobotProject/TestSuiteFolder/TestFileToMove.robot" 
    | get-property caption | equals "TestFileToMove.robot" | verify-true
