    get-item RobotProject | get-property "getItems().TreeItem[3].getText()" | equals "abc.robot" | verify-true
    get-item RobotProject | get-property "getItems().TreeItem[4].getText()" | equals "red.xml" | verify-true
    get-item RobotProject | get-property "getItems().TreeItem[5].getText()" | equals "TestFile.robot" | verify-true
}
