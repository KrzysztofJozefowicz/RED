Content-Type: text/ecl
Entry-Name: .content

with [get-view "Project Explorer" | get-tree] {
    select "RobotProject/TestFile.robot" | double-click
    select "RobotProject/TestFile.robot" | get-menu "Open With/Robot Table Editor" | click
}
with [get-editor "TestFile.robot" -type "Robot Table Editor"] {
    get-tab-folder | get-tab-item Settings | click
    get-link "Section is not yet defined, do you want to create it?" | click
}
get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | key-type F2
with [get-window "Rename Resource"] {
    with [get-editbox -after [get-label "New name:"]] {
        key-type Left -times 6
        set-text "TestFile1.robot"
    }
    get-button OK | click
}
get-window "Save Resource" | get-button Yes | click
get-tab-folder -index 1 | get-property activePage | equals "*TestFile.robot" | verify-true
