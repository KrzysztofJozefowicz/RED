with [get-editor "TestFile.robot"] {
    //open editor on value
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Right
        key-type Enter
    }
    with [get-editbox -after [get-label Variables] -index 1] {
        //enter text by keyboard
        set-text "item=value"
        key-type Enter -times 2
        
        //add key
        set-text "only item"
        key-type Enter -times 2
        
        //add only value
        set-text "=only value"
        key-type Enter -times 2
        
        //open content assist
        key-type "M1+SPACE"
        
        //add ${True}
        key-type Up
        wait 300
        key-type Up -times 4
        key-type Enter
        
		//add value for $TRUE
        type-text "=false"
        key-type Enter
    }
}

