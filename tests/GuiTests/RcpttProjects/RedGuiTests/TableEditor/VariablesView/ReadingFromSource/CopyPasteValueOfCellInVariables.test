get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | double-click

get-editor "TestFile.robot" | get-text-viewer | set-text "*** Variable ***\n"
    + "${var}    0    #comment0\n"
    + "${var1}    1    #comment1\n"
    + "${var2}    2    #comment2\n"
