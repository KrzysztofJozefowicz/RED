with [get-view "Project Explorer" | get-tree] {
    select RobotProject | double-click
    select "RobotProject/TestFile.robot" | double-click
}
// add Variable section and one variable ${text} in Source
with [get-editor "TestFile.robot"] {
    with [get-text-viewer] {
        set-caret-pos 1 19
        key-type Enter
        type-text "*"
        key-type "M1+SPACE"
        key-type Down -times 3
        key-type Enter
        type-text "${text}"
        key-type Enter
        key-type "M1+s"
    }
    get-tab-folder | get-tab-item Variables | click
}
// go to Variable section through Outline view
get-view Outline | get-tree | select "TestFile.robot/Variables/text" | double-click
with [get-editor "TestFile.robot"] {
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Down
        key-type Enter -times 2
    }
    // add second variable ${var24} with '...add new scalar'
    with [get-editbox -after [get-label Variables] -index 1] {
        set-text "${var24}"
    }
	// add next three variables - it should be var25, var26, var27 
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Right
        key-type Left
        key-type Down
        key-type Enter
        key-type Down
        key-type Enter
        key-type Down
        key-type Enter
        key-type Down
        key-type Enter -times 2
    }
    // add list variable @{tab} 
    with [get-editbox -after [get-label Variables] -index 1] {
        set-text "@{tab}"
    }
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Down
        key-type Enter
        key-type "M2+7" "&"
    }
    // add dictonary variable &{dict}
    with [get-editbox -after [get-label Variables] -index 1] {
        set-text "&{dict}"
    }
}
// add scalar variable - it should be var28 - and values for every variable
with [get-editor "TestFile.robot"] {
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Down
        key-type Enter
        key-type Up -times 7
        key-type Right
        type-text 1
    }
    get-editbox -after [get-label Variables] -index 1 | key-type Down
    with [get-editbox -after [get-label Variables] -index 1] {
        type-text 2
    }
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Down
        key-type Enter
    }
    with [get-editbox -after [get-label Variables] -index 1] {
        type-text 3
    }
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Down
        key-type Enter
    }
    with [get-editbox -after [get-label Variables] -index 1] {
        type-text 4
    }
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Down
        key-type Enter
    }
    with [get-editbox -after [get-label Variables] -index 1] {
        type-text 5
    }
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Down
        key-type Enter
    }
    with [get-editbox -after [get-label Variables] -index 1] {
        type-text "1"
        key-type Enter
        type-text "2"
        key-type Enter
        key-type "TRAVERSE_TAB_NEXT"
    }
     with [get-canvas -after [get-label Variables] -index 1] {
        key-type Down
        key-type Left
        key-type Enter
    }
    with [get-editbox -after [get-label Variables] -index 1] {
        type-text "d=1"
        key-type Enter
        key-type "TRAVERSE_TAB_NEXT"
	}
    with [get-canvas -after [get-label Variables] -index 1] {
        key-type Down
