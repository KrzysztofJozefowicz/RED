    	// add Log to Test setup
        key-type Down -times 2
        key-type Right
        key-type "M2+l"
    }
    with [get-editbox -index 2] {
        type-text og
        key-type Enter
    }
    /// add value
    with [get-editbox -index 2] {
        type-text txt
        key-type Enter
    }
    //add "a" to test Tempalte
