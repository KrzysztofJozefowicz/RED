// open testcase, open KW table editor
get-view "Project Explorer" | get-tree | select "REDProject/new_test.robot" | double-click
with [get-editor "new_test.robot"] {
    get-tab-folder | get-tab-item Keywords | click
// add keyword between 2 keywords
    get-tree | select "Check if starts" | get-menu "Add new keyword" | click
}
// verify if added
get-editor "new_test.robot" | get-tree | get-property "getItems().TreeItem[1].getText()" | equals Keyword | verify-true
// remove middle KW
with [get-editor "new_test.robot" | get-tree] {
    select Keyword | get-menu "Row/Delete row" | click
}
// verify if removed
get-editor "new_test.robot" | get-tree | get-property itemCount | equals 3 | verify-true
