    + "    None Shall Pass    who\n"
    + "    [Return]    6\n"
    + "kw3\n"
    + "    Call Method    object    method_name\n"
    + "Keyword\n"
    + "    [Documentation]\n"
    + "    \\\n"
    + "Keyword 1\n"
    + "    [Documentation]" | verify-true
    
    
