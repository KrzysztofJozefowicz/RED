//open editor
get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | double-click
get-editor "TestFile.robot" | get-tab-folder | get-tab-item Keywords | click
with [get-editor "TestFile.robot"] {
    get-link "Section is not yet defined, do you want to create it?" | click
}  


//add keyword by navigation by arrows and accpet by Enter
add-keyword-with-elements-by-arrows -keyword_name "Example 1" -elements [list "@{list} =" "Create List"  "first" "second" "third"] -script_name "TestFile.robot"
add-row-to-keyword-by-arrows-end-enter -keyword_name "Example 1" -keyword_index 0 -elements [list "Length Should Be" "${list}" "3"] -script_name "TestFile.robot"
add-row-to-keyword-by-arrows-end-enter -keyword_name "Example 1" -keyword_index 0 -elements [list "Log Many" "@{list}"] -script_name "TestFile.robot"

//add keyword by double click
add-keyword-with-elements-by-double-click -keyword_name "Open Login Page" -elements [list "Open Browser" "http://host/login.html" "" "" "" "" "this is for selenium"]  -script_name "TestFile.robot"
add-row-to-keyword-by-double-click-on-cell -keyword_name "Open Login Page" -keyword_index 0 -elements [list "Title Should Be"  "Login Page"] -command_index 0  -script_name "TestFile.robot"

//add keywords with arguments
add-keyword-with-args-by-arrows -keywords_name "Three aruments" -arguments [list "${arg1}" "${arg2}" "${arg3}" "" "" "comment"] -script_name "TestFile.robot"
add-row-to-keyword-by-arrows-end-enter -keyword_name "Three aruments"  -keyword_index 0 -elements [list "Log" "1st argument: ${arg1}"] -script_name "TestFile.robot"
add-row-to-keyword-by-arrows-end-enter -keyword_name "Three aruments"  -keyword_index 0 -elements [list "Log" "2nd argument: ${arg2}"] -script_name "TestFile.robot"
add-row-to-keyword-by-arrows-end-enter -keyword_name "Three aruments"  -keyword_index 0 -elements [list "Log" "3rd argument: ${arg3}"] -script_name "TestFile.robot"
add-keyword-by-double-click -keywords_list [list "Empty Keyword 1" "Empty keyword 2"]-script_name "TestFile.robot"
add-keyword-with-elements-by-double-click -keyword_name "WWW sum 2 digits" -elements [list "${b} =" "set variable" "${1}"] -script_name "TestFile.robot"


//navigate by arrows and delete cells
with [get-editor "TestFile.robot" | get-tree] {
    select "WWW sum 2 digits/${b} =" | key-type Up
    select "WWW sum 2 digits" | key-type Up
    select "Empty keyword 2" | key-type Up
    select "Empty Keyword 1" | key-type Up
    select "Three aruments/..." | key-type Up
    select [get-item "Three aruments" | get-item Log -index 2] | key-type Up
    select [get-item "Three aruments" | get-item Log -index 1] | key-type Up
    select "Three aruments/Log" | key-type Up
    select "Three aruments" | key-type Up
    select "Open Login Page/..." | key-type Up
    select "Open Login Page/Title Should Be" | key-type Up
    select "Open Login Page/Open Browser" | key-type Up
    select "Open Login Page" | key-type Up
    select "Example 1/..." | key-type Up
    select "Example 1/Log Many" | key-type Left
    key-type Del
    key-type Down
    select "Example 1/..." | key-type Down
    select "Open Login Page" | key-type Down
    select "Open Login Page/Open Browser" | key-type Left
    key-type Del
    key-type Right -times 6
    key-type Del
    key-type Down
    select "Open Login Page/Title Should Be" | key-type Down
    select "Open Login Page/..." | key-type Down
    select "Three aruments" | key-type Left -times 4
    key-type Del
    key-type Down
    select "Three aruments/Log" | key-type Down
    select [get-item "Three aruments" | get-item Log -index 1] | key-type Left
    key-type Del
    key-type Down
    select [get-item "Three aruments" | get-item Log -index 2] | key-type Down
    select "Three aruments/..." | key-type Down
    select "Empty Keyword 1" | key-type Left
    key-type Del
}


get-editor "TestFile.robot" | get-tree | get-table-data | eq [read-csv-file [uri-from-path  [format "%s/TableEditor/KeywordsView/Verification/DeletingCellsInKeywords.csv" $RCPTT_project_path] ] ]|
		 assert-true -message "Data in table does not match input file"
		 
