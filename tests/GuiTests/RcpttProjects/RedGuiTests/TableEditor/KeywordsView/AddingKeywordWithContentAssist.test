Content-Type: text/ecl
Entry-Name: .content

get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | select "RobotProject/TestFile.robot" 
    | double-click
with [get-editor "TestFile.robot"] {
    get-tab-folder | get-tab-item Keywords | click
    get-link "Section is not yet defined, do you want to create it?" | click
    with [get-tree] {
    	get-editor "TestFile.robot" | get-tab-folder | get-tab-item Keywords | click
        get-item "...add new keyword"  | double-click
        select Keyword | activate-cell-edit
        with [get-editbox] {
            set-text "New Kyword"
            key-type Enter
        }
        apply-cell-edit -deactivate
        get-item "New Kyword/..." |double-click
        select "New Kyword/" | activate-cell-edit
        with [get-editbox] {
            key-type "M1+SPACE"
            key-type Down -times 18
            key-type Enter
        }
        apply-cell-edit -deactivate
        key-type Right
        key-type "NUMPAD_5" 5
        select "New Kyword/Convert To Hex" | activate-cell-edit -column 1
        with [get-editbox] {
            set-text 55
            key-type "NUMPAD_ENTER" "\\u000D"
        }
        apply-cell-edit -deactivate
        key-type "M1+s"
    }
}

with [get-editor "TestFile.robot" | get-tree] {
    get-property itemCount | equals 2 | verify-true
    get-property "getItems().TreeItem[0].getText()" | equals "New Kyword"  | verify-true
    get-property "getItems().TreeItem[1].getText()" | equals "...add new keyword" | verify-true
    get-item "New Kyword" | get-property childCount | equals 2 | verify-true
    get-item "New Kyword/Convert To Hex" | get-property "columns[0]" | equals "Convert To Hex" | verify-true
    get-item "New Kyword/Convert To Hex" | get-property "columns[1]" | equals 55 | verify-true
    get-item "New Kyword/Convert To Hex" | get-property "columns[2]" | equals "   " | verify-true
    get-item "New Kyword/Convert To Hex" | get-property "columns[3]" | equals "" | verify-true
    get-item "New Kyword/Convert To Hex" | get-property "columns[4]" | equals "" | verify-true
    get-item "New Kyword/Convert To Hex" | get-property "columns[5]" | equals "" | verify-true
    get-item "New Kyword/Convert To Hex" | get-property "columns[6]" | equals "" | verify-true
}

add-keyword-with-elements-by-double-click -keyword_name "WWW sum 2 digits" -elements [list "${b} =" "set variable" "${1}"] -script_name "TestFile.robot"
add-row-to-keyword-by-double-click-on-cell -keyword_name "WWW sum 2 digits" -keyword_index 0 -elements [list "${1} =" "set variable" "${2 }"] -script_name "TestFile.robot"


with [get-editor "TestFile.robot" | get-tree] {
    get-item "...add new keyword"  | double-click
    select Keyword | activate-cell-edit
    with [get-editbox] {
        set-text "Check keyword"
        key-type Enter
    }
    apply-cell-edit -deactivate
    get-item "Check keyword/..." | double-click
    select "Check keyword/" | activate-cell-edit
    with [get-editbox] {
        key-type "M1+SPACE"
        key-type End
        key-type Enter
    }
}

get-editor "TestFile.robot" | get-tree | apply-cell-edit -deactivate
get-editor "TestFile.robot" | get-tree | get-item "Check keyword/WWW sum 2 digits" | get-property caption 
    | equals "WWW sum 2 digits" | verify-true
