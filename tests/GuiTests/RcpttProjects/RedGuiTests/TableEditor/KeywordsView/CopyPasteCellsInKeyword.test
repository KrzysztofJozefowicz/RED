// test: copy one cell to the same body of KW

// open suite and change to KW table
get-view "Project Explorer" | get-tree | select "MediumRobotTest/Script.robot" | double-click
with [get-editor "Script.robot"] {
    with [get-text-viewer] {
        set-caret-pos 30 8
    }
    get-tab-folder | get-tab-item Keywords | click
}
//set focus on item to copy from KW from Outline
get-view Outline | get-tree | select "Script.robot/Keywords/REVERSE MY LIST/Create List"
with [get-editor "Script.robot" | get-canvas -after [get-label "User Keywords"] -index 1] {
	// use RMB menu to copy element
    get-menu Copy | click
    // go 2 rows down
    key-type Down -times 2
    // paste item
    get-menu Paste | click
    key-type "M1+s"
}
// move cursor one row down on ..., add new line by Enter, bast the same cell by ctrl+v
get-eclipse-window | key-type "TRAVERSE_ARROW_NEXT"
get-eclipse-window | key-type Down
with [get-editor "Script.robot"] {
    with [get-canvas -after [get-label "User Keywords"] -index 1] {
        key-type Down
        key-type Enter
        key-type "M1+v"
    }

}



// move one down to ... create new line by Enter, ADD ROW by ctrl+shift+v RMB menu
with [get-editor "Script.robot"] {
    with [get-canvas -after [get-label "User Keywords"] -index 1] {
        key-type Down
        key-type Enter
        key-type "M1+M2+v"
        key-type Down
        get-menu "Row/Paste row" | click
    }
//valiate Source
    get-tab-folder | get-tab-item Source | click
}
