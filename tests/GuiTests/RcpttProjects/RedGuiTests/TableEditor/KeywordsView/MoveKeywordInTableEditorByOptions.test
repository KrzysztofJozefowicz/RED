// open file with Keywords and open Keyword table edit

get-view "Project Explorer" | get-tree | select "REDProject/new_test.robot" | double-click
with [get-editor "new_test.robot"] {
    with [get-tab-folder] {
        get-tab-item Keywords | click
    }
    // click on fist Keyword and move it down by mouse option
    get-tree | select "Compare Two Strings" | get-menu "Row/Move down" | click
}
// validate if moved
get-editor "new_test.robot" | get-tree | get-property "getItems().TreeItem[1].getText()" | equals "Compare Two Strings" 
    | verify-true
// move up by mouse option
get-editor "new_test.robot" | get-tree | select "Compare Two Strings" | get-menu "Row/Move up" | click
// verify if moved
get-editor "new_test.robot" | get-tree | get-property "getItems().TreeItem[0].getText()" | equals "Compare Two Strings" 
    | verify-true
