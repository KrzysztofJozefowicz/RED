// open test case, and go to Table editor Test Cses

get-view "Project Explorer" | get-tree | select "MediumRobotTest/Script.robot" | double-click
with [get-editor "Script.robot"] {
    with [get-tab-folder] {
        get-tab-item "Test Cases" | click
    }
    // move TC one down by menu
    get-tree | select "CAPITAL LETTER" | get-menu "Row/Move down" | click
}


// verify if moved one down

wait 500
get-editor "Script.robot" | get-tree | get-item "CAPITAL LETTER" | get-property index | equals 1 | verify-true
// move up by menu
get-editor "Script.robot" | get-tree | select "CAPITAL LETTER" | get-menu "Row/Move up" | click
// verify if moved up
wait 500
get-editor "Script.robot" | get-tree | get-item "CAPITAL LETTER" | get-property index | equals 0 | verify-true
   

    
