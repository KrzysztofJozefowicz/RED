Content-Type: text/ecl
Entry-Name: .content

// open file, set editor to Test Cases

get-view "Project Explorer" | get-tree | select "RobotProject/TestFile.robot" | double-click
with [get-editor "TestFile.robot"] {
    get-tab-folder | get-tab-item "Test Cases" | click
}
// create one test case     
with [get-editor "TestFile.robot" | get-tree] {
		    select "...add new test case" | key-type Enter
		    select "case" | activate-cell-edit
		    get-editbox | key-type Enter
		    apply-cell-edit -deactivate
		    key-type Right
		    key-type Down
		    select "case/..." | key-type Enter
		    select "case/" | activate-cell-edit

  }      

// pres C and take Call Method from content assist
with [get-editor "TestFile.robot" | get-tree] {
    select "case/" | double-click
    select "case/" | activate-cell-edit
    with [get-editbox] {
        set-text C
        key-type "M1+SPACE"
        key-type Enter

    }
    apply-cell-edit -deactivate
}

// check if Call Method is present in editor
get-editor "TestFile.robot" | get-tree | get-item "case/Call Method" | get-property caption | equals "Call Method" 
    | verify-true
