//run test with failing tests
with [get-tab-folder -index 4] {
    get-tab-item "Execution View" | click
}

get-view "Project Explorer" | get-tree | select "ExecutionViewProject/normal_cases.robot" 
    | select "ExecutionViewProject/normal_cases.robot" | get-menu "Run As/1 Robot Test" | click

wait 500
//check how many test were executed
with [get-view "Execution View"] {
    get-label "Passed: 7" | get-property caption | equals "Passed: 7" | verify-true
    get-label "Failed: 4" | get-property caption | equals "Failed: 4" | verify-true
}
//show only failed cases
get-view "Execution View" | get-button "Show Failures Only" | click

get-view "Execution View" | get-tree | get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
    | get-property childCount | equals 4 | verify-true
    
with [get-view "Execution View" | get-tree] {
    get-item "ExecutionViewProject \\(.* s\\)/Normal Cases \\(.*\\)" 
        | get-property "getItems().TreeItem[0].getText()" | equals "failing test \(0\.0\d\d? s\)" | verify-true
    get-item "ExecutionViewProject \\(.* s\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[1].getText()" | equals "keyword are lowercase from resource \(0\.0\d\d? s\)" 
        | verify-true
    get-item "ExecutionViewProject \\(.* s\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[2].getText()" | equals "double loop \(0\.0\d\d s\)" | verify-true
    get-item "ExecutionViewProject \\(.* s\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[3].getText()" | equals "test that containf FAIL command \(0\.0\d\d? s\)" 
        | verify-true
}
 //show all   
get-view "Execution View" | get-button "Show Failures Only" | click
with [get-view "Execution View" | get-tree] {
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" | get-property childCount | equals 11 
        | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[0].getText()" | equals "CAPITAL LETTER \(0\.0\d\d? s\)" | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[1].getText()" | equals "test1 \(0\.0\d\d? s\)" | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[2].getText()" | equals "test2 \(0\.0\d\d? s\)" | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[3].getText()" | equals "failing test \(0\.0\d\d? s\)" | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[4].getText()" | equals "convert to lower case \(0\.0\d\d? s\)" | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[5].getText()" | equals "make dict \(0\.0\d\d? s\)" | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[6].getText()" | equals "check resource \(0\.0\d\d? s\)" | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[7].getText()" | equals "keyword are lowercase from resource \(0\.0\d\d? s\)" 
        | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[8].getText()" | equals "double loop \(0\.0\d\d? s\)" | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[9].getText()" | equals "test that containf FAIL command \(0\.0\d\d? s\)" 
        | verify-true
    get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
        | get-property "getItems().TreeItem[10].getText()" | equals "time with sleep command \(2.\d\d?\d? s\)" | verify-true
}
//show only failing again
with [get-view "Execution View" | get-button "Show Failures Only"] {
    click
}
get-view "Execution View" | get-tree | get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
    | get-property childCount | equals 4 | verify-true

//show all tests
with [get-view "Execution View" | get-button "Show Failures Only"] {
    click
}    
get-view "Execution View" | get-tree | get-item "ExecutionViewProject \\(.*\\)/Normal Cases \\(.* s\\)" 
    | get-property childCount | equals 11 | verify-true

