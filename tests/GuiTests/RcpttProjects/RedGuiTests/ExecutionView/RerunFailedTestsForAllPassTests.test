get-view "Project Explorer" | get-tree | select "ExecutionViewProject/all_pass_tests.robot" 
	| get-menu "Run As/1 Robot Test" | click
wait-for-console   
    
get-view "Execution View" | get-button "Rerun Failed Tests Only" | click
wait-for-console  


try -command{
	get-view Console | get-text-viewer | get-property text 
	    | matches "Command: .*\n"
	    + "Suite Executor: Robot Framework .*\n"
	    + "\[ ERROR \] Collecting failed tests from '.*output.xml' failed: All tests passed.\n\n"
	    + "Try --help for usage information." | verify-true
}-catch{
	get-view Console | get-text-viewer | get-property text 
	    | matches "\[ ERROR \] Collecting failed tests from '.*output.xml' failed: All tests passed.\n\n"
	    + "Try --help for usage information.\n" | verify-true
	    "Command: .*\n"
	    + "Suite Executor: Robot Framework .*"
	    
}    
get-view "Execution View" | get-tree | get-property itemCount | equals 0 | verify-true
    
