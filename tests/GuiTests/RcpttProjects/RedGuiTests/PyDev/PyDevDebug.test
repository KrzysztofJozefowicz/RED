// set PyDev preferences
get-preferences-menu | click

with [get-window Preferences] {
    get-tree | select PyDev
    get-button "Check preferred Eclipse settings for PyDev on editor open" | uncheck
    get-tree | select "PyDev/Interpreters"
    get-button "Notify when a Python project has no interpreter?" | uncheck
    get-button "Notify when a Jython project has no interpreter?" | uncheck
    get-button "Notify when an IronPython project has no interpreter?" | uncheck
    get-button "Check initial consistency (in 1 minute) after startup?" | uncheck
    get-button "Check consistency when related files (i.e.: .py, .pth) in the PYTHONPATH change?" | uncheck
    get-button OK | click
}
get-preferences-menu | click
//PyDev interpreter preferences
get-window Preferences | get-tree | select "PyDev/Interpreters/Python Interpreter"

if [get-window Preferences | get-button Remove | get-property enablement -raw | eq true]
{
	get-window Preferences | get-button Remove | click
	
}
with [get-window Preferences] {
	    get-tree | select "PyDev/Interpreters/Python Interpreter"
	    get-button "Quick Auto-Config" | click
		get-button Apply | click
 
	}
get-window Preferences | get-button OK | click



get-view "Project Explorer" | get-tree | select "PyDevRobotDebug/test.robot" | double-click
with [get-editor "test.robot" | get-left-ruler] {
    with [get-ruler-column AnnotationColumn] {
        click-ruler -line 10
        click-ruler -line 10 -button Right
    }
    get-menu "Toggle Breakpoint" | click
}
get-view "Project Explorer" | get-tree | select "PyDevRobotDebug/myLib.py" | double-click

get-button "Open Perspective" | click
with [get-window "Open Perspective"] {
    get-table | select PyDev
    get-button OK | click
}


get-button "Open Perspective" | click
get-window "Open Perspective" | get-table | select Debug | double-click
with [get-view Breakpoints] {
    click
    get-tree | get-menu "Import Breakpoints..." | click
}
set-dialog-result File "D:\\PyDevMyLibBreakPoints.bkpt"
with [get-window "Import Breakpoints"] {
    get-button "Browse..." | click
    get-button "Update existing breakpoints" | check
    get-button "Next >" | click
    get-button Finish | click
}

//
get-menu "Run/Debug Configurations..." | click
with [get-window "Debug Configurations"] {
    get-tree | select Robot | double-click
    get-group Project | get-button "Browse..." | click
    get-window "Select project" | get-tree | select PyDevRobotDebug | double-click
    get-group "Test Suite(s)" | get-button "Browse..." | click
    get-window "Select test suite" | get-tree | select PyDevRobotDebug | double-click
    with [get-group "Test Suite(s)"] {
        get-button "Select All" | click
        get-button "Browse..." | click
    }
    with [get-window "Select test suite"] {
        get-tree | select "PyDevRobotDebug/test.robot"
        get-button OK | click
    }
    get-tab-folder | get-tab-item Executor | click
}





get-window "Debug Configurations" | get-group "Executable file" | get-editbox 
    -after [get-label "Setup executable file which will be used to run Robot Framework tests instead of interpreter selected above"] 
    | click-text
set-dialog-result File "/usr/bin/python"
with [get-window "Debug Configurations"] {
    with [get-group "Executable file"] {
        get-button "Browse..." | click
        get-editbox -after [get-label "Additional executable file arguments:"] | set-text "/home_local/root/jenkins/workspace/RED_tmp2/RobotUserScripts/runPyDev.py"
    }
    get-button Apply | click
    get-button Close | click
}

get-menu "Pydev/Start Debug Server" | click

get-editor "test.robot" | click
get-menu "Run/Debug" | click
with [get-button "Step Over (F6)"] {
    click
    click
}
get-editor myLib | click
get-editor "test.robot" | click
get-button "Terminate (M1+F2)" | click
